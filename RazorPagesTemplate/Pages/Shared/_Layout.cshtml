<!DOCTYPE html>
<html lang="en" data-bs-theme="auto">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - RazorPagesTemplate</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/mystyle.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/css@3.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/css/sign-in.css" asp-append-version="true"/>
</head>
<body>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/color-modes.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>